{
  "name": "apigateway",
  "version": "0.0.4",
  "description": "This is a component for alibaba cloud apigateway",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "publish": "npm run build && s cli platform publish",
    "build": "tsc",
    "esbuild": "esbuild lib/index.js --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --external:electron --outfile=dist/index.js",
    "postbuild": "npm run doc && npm run esbuild && cp -r ./locales ./dist/locales",
    "doc": "npx typedoc src/index.ts --json doc/doc.json --out doc"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.10",
    "@serverless-devs/core": "latest",
    "i18n": "^0.13.2",
    "js-yaml": "^4.0.0",
    "loadsh": "0.0.4",
    "lodash.get": "^4.4.2",
    "tty-table": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "esbuild": "^0.14.2",
    "eslint": "^7.7.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.7"
  }
}
